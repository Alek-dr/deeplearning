    "import math\n",
    "import random\n",
    "\n",
    "\n",
    "random.seed()\n",
    "\n",
    "n = 10000000\n",
    "\n",
    "dots = 0.\n",
    "for i in range(n):\n",
    "    x = random.random()\n",
    "    y = random.random()\n",
    "    z = (x,y)\n",
    "\n",
    "    if x**2+y**2 <= 1:\n",
    "#         print z\n",
    "        dots += 1\n",
    "    else:\n",
    "        del z\n",
    "        \n",
    "print \"Pi\", 4*dots/n"
   ]
